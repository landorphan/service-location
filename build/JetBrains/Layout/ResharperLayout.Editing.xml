      <Entry DisplayName="Methods">
         <Entry.Match>
            <And>
               <Kind Is="Method" />
               <Not>
                  <Kind Is="Destructor" />
               </Not>
               <Not>
                  <HandlesEvent />
               </Not>
            </And>
         </Entry.Match>
         <Entry.SortBy>
            <Access Order="Public Internal ProtectedInternal Protected Private" />
